{
  "name": "express-gatekeeper-core",
  "version": "1.0.0",
  "engines": {
    "node": ">=11.*"
  },
  "main": "build/index.js",
  "typings": "build/types/src/types.d.ts",
  "scripts": {
    "test": "npx jest",
    "test:watch": "npx jest --watchAll",
    "test:e2e": "npx jest --config=./jest.e2e.config.js",
    "coverage": "npx jest --coverage",
    "lint": "npx tslint --force --fix \"./src/**/*.ts\" \"./tests/**/*.ts\"",
    "build": "webpack --display-error-details",
    "example:auth": "nodemon --exec ts-node example/authorization/app.ts --resolveJsonModule --watch src example",
    "example:proxy": "nodemon --exec ts-node example/proxy/app.ts --resolveJsonModule --watch src example"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/body-parser": "^1.19.0",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.3",
    "@types/express-http-proxy": "^1.5.12",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.12",
    "@types/object-path": "^0.11.0",
    "@types/path-to-regexp": "^1.7.0",
    "@types/url-parse": "^1.4.3",
    "@types/supertest": "^2.0.8",
    "@types/yamljs": "^0.2.30",
    "autoprefixer": "7.1.6",
    "babel-jest": "^24.8.0",
    "body-parser": "^1.19.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "jest": "^24.7.1",
    "nodemon": "^2.0.2",
    "pre-push": "^0.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^8.6.2",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "^1.1.2",
    "webpack": "3.8.1",
    "webpack-cli": "^3.3.9",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "deep-equal": "^1.0.1",
    "express-http-proxy": "^1.6.0",
    "expression-eval": "^2.1.0",
    "object-path": "^0.11.4",
    "router-params": "^2.0.2",
    "url-parse": "^1.4.7"
  }
}
